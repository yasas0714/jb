<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>register.Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>authentication</servlet-name>
        <servlet-class>authentication.authentication</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addproduct</servlet-name>
        <servlet-class>addproduct.addproduct</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>userorder</servlet-name>
        <servlet-class>userorder.userorder</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>userorder1</servlet-name>
        <servlet-class>userorder1.userorder1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>authentication</servlet-name>
        <url-pattern>/authentication</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addproduct</servlet-name>
        <url-pattern>/addproduct</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>userorder</servlet-name>
        <url-pattern>/userorder</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>userorder1</servlet-name>
        <url-pattern>/userorder1</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
